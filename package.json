    "typescript": "^2.3.2",
    "concurrently": "^3.4.0"
  },
  "dependencies": {
    "@types/body-parser": "^1.16.3",
    "@types/express": "^4.0.35",
    "@types/request": "0.0.43",
    "@types/request-promise-native": "^1.0.5",
    "body-parser": "^1.17.1",
    "cookie-parser": "^1.4.3",
    "express": "^4.15.2",
    "morgan": "^1.7.0",
